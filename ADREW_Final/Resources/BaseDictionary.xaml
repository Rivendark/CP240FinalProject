<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Brushes -->
    <SolidColorBrush x:Key="PrimaryBackgroundBrush">#1D1D1D</SolidColorBrush >
    <SolidColorBrush x:Key="AlternatingBackgroundBrush">#1A1A1A</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryBorderBrush">#000</SolidColorBrush >
    <SolidColorBrush x:Key="SelectedBackgroundBrush">#F60</SolidColorBrush>
    <SolidColorBrush x:Key="TransparentLabelColor">#CC141414</SolidColorBrush>
    <!-- Colors -->
    <Color x:Key="BorderColor">#FF000000</Color>
    <Color x:Key="BackgroundColor">#FF1D1D1D</Color>
</ResourceDictionary>